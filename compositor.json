    "layout": {
      "maxWidth": 768
    }
  },
  "content": [
    {
      "component": "nav",
      "links": [
        {
          "href": "https://github.com/timoweiss/compositor",
          "text": "GitHub"
        }
      ]
    },
    {
      "component": "header",
      "heading": "Test",
      "subhead": "Sub",
      "children": [
        {
          "component": "ui/TweetButton",
          "text": "compositor: ",
          "url": null
        },
        {
          "component": "ui/GithubButton",
          "user": "timoweiss",
          "repo": "compositor"
        }
      ],
      "slug": "slug Test",
      "text": "text"
    },
    {
      "component": "article",
      "metadata": {
        "source": "github.readme"
      },
      "html": "\n"
    },
    {
      "component": "footer",
      "links": [
        {
          "href": "https://github.com/timoweiss/compositor",
          "text": "GitHub"
        },
        {
          "href": "https://github.com/timoweiss",
          "text": "timoweiss"
        }
      ]
    }
  ]
}