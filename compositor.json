      "nav": "nav/BasicNav",
      "header": "header/LightBannerHeader",
      "article": "article/ReaderArticle",
      "footer": "footer/BasicFooter"
    },
    "fontFamily": "Athelas, Georgia, serif",
    "heading": {
      "fontWeight": 400,
      "textTransform": "uppercase",
      "letterSpacing": "0.1em"
    },
    "colors": {
      "text": "#121212",
      "background": "#fff",
